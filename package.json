  "scripts": {
    "storybook": "start-storybook -p 6006 -s ./src",
    "prebuild:storybook": "npm run test:generate-output",
    "build:storybook": "build-storybook -c .storybook -o build/",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test": "jest -u",
    "build": "npm run clean && webpack --mode=production --node-env=production",
    "build:dev": "npm run clean && webpack",
    "clean": "rm -rf lib",
    "copy-files": "cp -R src/assets lib/assets",
    "lint": "eslint --fix src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src --config ./.prettierrc",
    "chromatic": "npx chromatic --project-token=e5c345734579",
    "release:patch": "npm run build && git tag -l | xargs git tag -d && git checkout releases && npm version patch && git push origin && git push origin --tags && npm publish",
    "release:minor": "npm run build && git tag -l | xargs git tag -d && git checkout releases && npm version minor && git push origin && git push origin --tags && npm publish",
